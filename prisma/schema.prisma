generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id String @id @default(uuid())
  clerkId String @unique
  position String
  company String
  location String
  mode String
  status String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}